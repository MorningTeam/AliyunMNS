{
  "name": "morning/aliyunmns",
  "description": "Aliyun Message Service PHP SDK 1.3.5",
  "type": "library",
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "~6.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "files": [
      "mns-autoloader.php"
    ],
    "psr-0": {
      "AliyunMNS": "./"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
